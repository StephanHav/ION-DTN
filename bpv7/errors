cd doc; \
	mkdir -p man; \
	mkdir -p man/man1; \
	mkdir -p man/man3; \
	mkdir -p man/man5; \
	mkdir -p html; \
	mkdir -p html/man1; \
	mkdir -p html/man3; \
	mkdir -p html/man5; \
	gmake all ROOT=/opt; \
	cd ..; \
	for PF in i86_64-fedora; \
		do \
			cd $PF; \
			mkdir -p bin; \
			mkdir -p lib; \
			gmake all ROOT=/opt PTRS=0; \
			cd ..; \
		done
